{
  "name": "slice MS",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "auto slice ms path",
  "browser_action": {
    "default_icon": "on.png",
    "default_title": "meta Check"
  },
  "background": {
    "scripts": [
    "background.js"
    ]
  },
  "permissions": [
  "activeTab"
  ],
  "content_scripts": [
  {
    "matches": ["http://*.jp/*"],
    "js": ["jquery-2.1.4.min.js", "mspath.js"],
    "run_at": "document_idle"
  }
  ]
}